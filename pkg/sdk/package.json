{
	"name": "@chainlink-registry/sdk",
	"version": "1.0.0",
	"description": "TypeScript SDK for Chainlink Registry Price API",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts,.tsx",
		"test": "vitest"
	},
	"keywords": [
		"chainlink",
		"price",
		"oracle",
		"defi",
		"typescript",
		"sdk"
	],
	"author": "JosÃ© Ribeiro",
	"license": "MIT",
	"packageManager": "pnpm@10.14.0",
	"devDependencies": {
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.0.0",
		"tsup": "^8.0.0",
		"typescript": "^5.0.0",
		"vitest": "^1.0.0"
	},
	"dependencies": {
		"cross-fetch": "^4.0.0"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	}
}
